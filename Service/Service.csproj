    <AssemblyName>CaptainPav.Images.Optimization.Service</AssemblyName>
    <RootNamespace>CaptainPav.Images.Optimization.Service</RootNamespace>
    <Version>1.2.0</Version>
    <Authors>Michael Pavlak</Authors>
    <Company>Visual Chimera, LLC</Company>
    <Description>Implementation of the optimization service utilizing azure storage and an abstract optimization service.</Description>
    <Copyright>Michael Pavlak</Copyright>
    <PackageLicenseUrl>https://github.com/spelltwister/CaptainPav.Images.Optimization/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/spelltwister/CaptainPav.Images.Optimization</PackageProjectUrl>
    <RepositoryUrl>https://github.com/spelltwister/CaptainPav.Images.Optimization</RepositoryUrl>
    <PackageTags>Azure Storage, Image Optimization</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
